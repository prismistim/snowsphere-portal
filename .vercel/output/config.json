{
  "version": 3,
  "routes": [
    {
      "src": "^/(?:(.+)/)?index(?:\\.html)?/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "src": "^/(.*)\\.html/?$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^((?!/api).*)$",
      "dest": "/ssr_/?__original_path=$1",
      "check": true
    }
  ],
  "overrides": {
    "index.html": {
      "path": ""
    }
  }
}